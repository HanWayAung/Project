@model IEnumerable<LuckDrawSystem.Models.WinnerList>

@{
    ViewBag.Title = "WinnerList";
}

<h2>WinnerList</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.WinningNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrizeType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.WinningNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrizeType)
        </td>
       
    </tr>
}

</table>
